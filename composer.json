{
	"name": "flynn314/seatemperature",
	"description": "Sea temperature data.",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "^8.3",
		"flynn314/webclient": "^1.0",
		"symfony/dom-crawler": "^7.2",
		"symfony/css-selector": "^7.2"
	},
	"require-dev": {
		"phpunit/phpunit": "11.5.2",
		"larapack/dd": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"Flynn314\\SeaTemperature\\": "src"
		}
	},
